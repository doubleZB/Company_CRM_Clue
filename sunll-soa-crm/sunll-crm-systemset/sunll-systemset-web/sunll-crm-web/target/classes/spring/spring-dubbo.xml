<?xml version="1.0" encoding="UTF-8"?>
<!--suppress ALL -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <!--引入配置属性文件 -->
    <context:property-placeholder location="classpath:config.properties"/>

    <!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
    <dubbo:application name="sunll-crm-web"/>

    <!-- 使用zookeeper广播注册中心暴露服务地址 -->

    <dubbo:registry address="zookeeper://47.93.118.247:2181?backup=47.94.42.84:2181,47.94.37.76:2181" group="qweqwe"/>
    <dubbo:annotation package="com.sunll.web.controller"/>
    <dubbo:reference interface="com.sunll.systemset.api.template.OpenTemplateService"
                     id="openTemplateService" check="false" timeout="100000" />
    <dubbo:reference interface="com.sunll.systemset.api.business.BusinessService"
                     id="businessService" check="false" timeout="100000" />
    <dubbo:reference interface="com.sunll.systemset.api.business.BusinessTypeService"
                     id="businessTypeService" check="false" timeout="100000" />
    <dubbo:reference interface="com.sunll.systemset.api.business.BusinessTypeFieldService"
                     id="businessTypeFieldService" check="false" timeout="100000" />
    <!-- account工程接口调用-->
    <dubbo:consumer timeout="100000"/>

</beans>